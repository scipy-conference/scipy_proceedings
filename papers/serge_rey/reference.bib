@article{longley2009a,
	Abstract = {Although viewed by business and commerce as successful
		  solutions,$\backslash$ngeodemographic profiling of
		  neighbourhoods has attracted
		  wide-ranging$\backslash$ncriticism in the academic
		  literature. This paper addresses some
		  specific$\backslash$nconcerns that arise because the
		  derivation of classifications is
		  rarely$\backslash$ntransparent and open to scrutiny or
		  challenge. The substantive focus of$\backslash$nthe
		  research reported in this paper is a nationwide
		  geodemographic$\backslash$nclassification of how people
		  engage with new information and$\backslash$ncommunication
		  technologies (ICTs). In response to the critique
		  of$\backslash$ngeodemographics as a `black box' technology,
		  we describe how the$\backslash$nclassification was opened
		  up to public scrutiny and how we conducted
		  a$\backslash$nmajor consultation exercise into the
		  reliability of its results. We$\backslash$nassess the
		  message of the 50,000+ searches and 3952 responses
		  collected$\backslash$nduring the consultation exercise, in
		  terms of possible systematic errors$\backslash$nin the
		  shape and detail of the classification. Unusually
		  for$\backslash$nInternet-based surveys, we also investigate
		  the likely reliability of$\backslash$nthe response
		  information received and identify ways in which the
		  outcome$\backslash$nof consultation might be used to
		  improve the classification. We believe$\backslash$nthat
		  this is the first-ever large-scale consultation survey of
		  the$\backslash$nvalidity and remit of a geodemographic
		  classification and that it may$\backslash$nhave wider
		  implications for the creation of geodemographic$\backslash$nclassifications.},
	Author = {Longley, Paul A. and Singleton, Alexander D.},
	Doi = {10.1080/13658810701704652},
	File = {:Users/knaaptime/Documents/Mendeley Desktop/Longley, Singleton - 2009 - Classification through consultation public views of the geography of the e-Society.pdf:pdf},
