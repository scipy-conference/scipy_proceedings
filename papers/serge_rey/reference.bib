@article{duque2011a,
	Abstract = {The p-regions problem involves the aggregation or
		  clustering of n small areas into p spatially contiguous
		  regions while optimizing some criteria. The main objective
		  of this article is to explore possible avenues for
		  formulating this problem as a mixed integer- programming
		  (MIP) problem. The critical issue in formulating this
		  problem is to ensure that each region is a spatially
		  contiguous cluster of small areas. We introduce three MIP
		  models for solving the p regions problem. Each model
		  minimizes the sum of dis- similarities between all pairs of
		  areas within each region while guaranteeing contigu- ity.
		  Three strategies designed to ensure contiguity are
		  presented: (1) an adaptation of the Miller, Tucker, and
		  Zemlin tour-breaking constraints developed for the
		  traveling salesman problem; (2) the use of ordered-area
		  assignment variables based upon an extension of an approach
		  by Cova and Church for the geographical site design prob-
		  lem; and (3) the use of flow constraints based upon an
		  extension of work by Shirabe. We test the efficacy of each
		  formulation as well as specify a strategy to reduce overall
		  problem size.},
	Author = {Duque, Juan C. and Church, Richard L. and Middleton, Richard S.},
	Doi = {10.1111/j.1538-4632.2010.00810.x},
