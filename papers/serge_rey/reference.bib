	Number = {1},
	Pages = {121--162},
	Title = {{Measures of Spatial Segregation}},
	Url = {http://doi.wiley.com/10.1111/j.0081-1750.2004.00150.x http://journals.sagepub.com/doi/10.1111/j.0081-1750.2004.00150.x},
	Volume = {34},
