	Issn = {0081-1750},
	Journal = {Sociological Methodology},
	Month = {aug},
	Number = {1},
	Pages = {195--221},
	Title = {{A Transition-Oriented Approach to Optimal Matching}},
	Url = {http://journals.sagepub.com/doi/10.1111/j.1467-9531.2011.01235.x},
	Volume = {41},
	Year = {2011},
	Bdsk-Url-1 = {http://journals.sagepub.com/doi/10.1111/j.1467-9531.2011.01235.x},
