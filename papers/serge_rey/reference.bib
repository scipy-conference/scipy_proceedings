	Mendeley-Groups = {boundary-harmonization},
	Month = {jul},
	Number = {3},
	Pages = {238--258},
	Title = {{A Spatially Disaggregated Areal Interpolation Model Using Light Detection and Ranging-Derived Building Volumes}},
