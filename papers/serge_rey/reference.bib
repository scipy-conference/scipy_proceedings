@article{assuncao2006,
	Abstract = {Regionalization is a classification procedure applied to
		  spatial objects with an areal representation, which groups
		  them into homogeneous contiguous regions. This paper
		  presents an efficient method for regionalization. The first
		  step creates a connectivity graph that captures the
		  neighbourhood relationship between the spatial objects. The
		  cost of each edge in the graph is inversely proportional to
		  the similarity between the regions it joins. We summarize
		  the neighbourhood structure by a minimum spanning tree
		  (MST), which is a connected tree with no circuits. We
		  partition the MST by successive removal of edges that link
		  dissimilar regions. The result is the division of the
		  spatial objects into connected regions that have maximum
		  internal homogeneity. Since the MST partitioning problem is
		  NP-hard, we propose a heuristic to speed up the tree
		  partitioning significantly. Our results show that our
		  proposed method combines performance and quality, and it is
		  a good alternative to other regionalization methods found
		  in the literature.},
	Author = {Assun{\c{C}}{\~{a}}o, R. M. and Neves, M. C. and C{\^{a}}mara, G. and {Da Costa Freitas}, C.},
	Doi = {10.1080/13658810600665111},
	File = {:Users/knaaptime/Documents/Mendeley Desktop/Assun{\c{C}}{\~{a}}o et al. - 2006 - Efficient regionalization techniques for socio‐economic geographical units using minimum spanning trees.pdf:pdf},
	Isbn = {1365-8816},
	Issn = {1365-8816},
	Journal = {International Journal of Geographical Information Science},
	Keywords = {Census data analysis,Constrained clustering,Graph partitioning,Optimization,Regionalization,Zone design},
	Month = {aug},
	Number = {7},
	Pages = {797--811},
	Title = {{Efficient regionalization techniques for socio‐economic geographical units using minimum spanning trees}},
	Url = {http://www.tandfonline.com/doi/abs/10.1080/13658810600665111},
	Volume = {20},
	Year = {2006},
	Bdsk-Url-1 = {http://www.tandfonline.com/doi/abs/10.1080/13658810600665111},
	Bdsk-Url-2 = {https://doi.org/10.1080/13658810600665111}}

