	Keywords = {Geodemographics,Information and communications technologies,e-Society},
	Month = {jun},
	Number = {6},
	Pages = {737--763},
	Title = {{Classification through consultation: public views of the geography of the e-Society}},
	Url = {http://www.tandfonline.com/doi/abs/10.1080/13658810701704652},
