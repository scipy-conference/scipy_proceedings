	Issn = {00167363},
	Journal = {Geographical Analysis},
	Month = {jan},
	Number = {1},
	Pages = {104--126},
	Title = {{The p-Regions Problem}},
	Url = {http://doi.wiley.com/10.1111/j.1538-4632.2010.00810.x},
	Volume = {43},
