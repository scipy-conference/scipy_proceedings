	Journal = {JSS Journal of Statistical Software},
	Keywords = {BANDIAGARA,MALI},
	Number = {16},
	Title = {{SPODT: An R Package to Perform Spatial Partitioning}},
	Url = {http://www.jstatsoft.org/},
