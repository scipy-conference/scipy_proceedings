	Author = {Phillips, JC and Braun, R and Wang, W and Gumbart, J and Tajkhorshid, E and Villa, E and Chipot, C and Skeel, RD and Kale, L and Schulten, K},
	Date-Added = {2016-05-31 17:48:27 +0000},
